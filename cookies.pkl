from selenium import webdriver
from selenium.webdriver.chrome.options import Options
import time
import pickle

# Skonfiguruj Chrome
options = Options()
options.add_argument("--start-maximized")

# Uruchom przeglÄ…darkÄ™
driver = webdriver.Chrome(options=options)

# PrzejdÅº do strony logowania Linkvertise
driver.get("https://publisher.linkvertise.com/login")

print("ğŸ” Zaloguj siÄ™ rÄ™cznie, masz 60 sekund...")
time.sleep(60)  # czas na rÄ™czne logowanie

# Po zalogowaniu â€“ zapisz cookies
cookies = driver.get_cookies()

with open("cookies.pkl", "wb") as f:
    pickle.dump(cookies, f)

print("âœ… Cookies zapisane jako cookies.pkl")

driver.quit()
